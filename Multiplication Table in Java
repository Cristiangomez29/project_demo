//Print the multiplication table of the number entered

import java.util.*;
public class Multiplication_Tables 
{
    public static void main(String args[])
    {
      Scanner in=new Scanner(System.in);
      System.out.println("Enter the number whose multiplication table you want to be printed");
      int n = in.nextInt();
      for (int i=1;i<=20;i++)
      {
        System.out.println(n+" x "+i+" = "+(n*i));
    }
  }
}
